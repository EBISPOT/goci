        <rdfs:label xml:lang="en">cytogenetic band</rdfs:label>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&ro;part_of"/>
                <someValuesFrom rdf:resource="&gwas;Chromosome"/>
            </Restriction>
        </rdfs:subClassOf>
        <efo:definition_editor xml:lang="en">A region of a chromosome, typically identified by karyotyping.</efo:definition_editor>
        <efo:definition_editor xml:lang="en">Tony Burdett</efo:definition_editor>
